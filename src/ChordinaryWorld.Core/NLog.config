<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <targets>
    <target layout="${message}" name="fromconsole" xsi:type="File" fileName="${basedir}/../../../ChordinaryWorld.Core/log.txt" />
    <target layout="${message}" name="fromweb" xsi:type="File" fileName="${basedir}/../ChordinaryWorld.Core/log.txt" />
  </targets>

  <rules>
    <logger name="*" writeTo="fromweb" />
  </rules>

</nlog>
